<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        style="@style/parent_match"
        tools:context=".view.HomeFragment">

        <ImageView
            android:id="@+id/iv_logo_home"
            android:layout_width="@dimen/iv_dimens"
            android:layout_height="@dimen/iv_dimens"
            android:layout_margin="@dimen/iv_margin"
            android:src="@drawable/vpn_logo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_test_vpn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hello_blank_fragment"
            app:layout_constraintEnd_toEndOf="@+id/iv_logo_home"
            app:layout_constraintStart_toStartOf="@+id/iv_logo_home"
            app:layout_constraintTop_toBottomOf="@id/iv_logo_home"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
